<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
	<typeAlias alias="parking" type="com.where2park.biz.parking.ParkingVO" />
	<typeAlias alias="rsv" type="com.where2park.biz.reservation.RsvVO" />
	<typeAlias alias="review" type="com.where2park.biz.review.ReviewVO" />
	<typeAlias alias="chat" type="com.where2park.biz.chat.ChatVO"/>		
	<typeAlias alias="user" type="com.where2park.biz.user.UserVO"/>
	<typeAlias alias="notice" type="com.where2park.biz.notice.NoticeVO"/>
	<typeAlias alias="wishList" type="com.where2park.biz.wishList.WishVO" />
	</typeAliases>

	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/parking-mapping.xml" />
		<mapper resource="mappings/reservation-mapping.xml" />
		<mapper resource="mappings/review-mapping.xml" />
		<mapper resource="mappings/chat-mapping.xml"/>
		<mapper resource="mappings/user-mapping.xml"/>
		<mapper resource="mappings/notice-mapping.xml"/>
		<mapper resource="mappings/wishList-mapping.xml"/>
	</mappers>
	
</configuration>
